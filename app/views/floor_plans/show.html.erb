<%= link_to 'Edit', edit_floor_plan_path(@floor_plan) %> |
<%= link_to 'Back', floor_plans_path %>

<div class="floor-plan-show">
  <%= render 'color_controls' %>

  <p class="floor-plan-title">
    <%= @floor_plan.title %>
  </p>

  <p class="floor-plan-image" data-file-name="<%= @floor_plan.title.dehumanize %>">
    <%= raw @floor_plan.image.file.read %>
  </p>
</div>
